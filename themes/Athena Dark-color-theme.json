				"meta.path",
				"support.other.namespace",
				"meta.at-rule.extend"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#11A0FF"
			}
		},
		{
			"name": "Class Names",
			"scope": [
				"entity.scope.name",
				"meta.function.parameter.typehinted",
				"support.class",
				"support.class.component.open",
				"support.class.component.close",
				"support.class.builtin",
				"variable.other.class",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#11A0FF"
			}
		},
		/*
			CONSTANTS & NUMBERS
		*/
		{
			"name": "Constants & Numbers",
			"scope": [
				"constant",
				"constant.character",
