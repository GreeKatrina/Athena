		{
			"name": "Library Functions",
			"scope": [
				"support.function",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0090F0"
			}
		},
		/*
			CONSTANTS & NUMBERS
		*/
		{
			"name": "Constants & Numbers",
			"scope": [
				"constant",
				"constant.character",
