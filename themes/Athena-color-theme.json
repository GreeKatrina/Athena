	"tokenColors": [
		/*
			NON-COLORED THINGS
		*/
		{
			"name": "Operators & Punctuation",
			"scope": [
				"keyword.operator.accessor",
				"keyword.operator.class",
				"keyword.operator.assignment",
				"keyword.operator.return-value",
				"keyword.operator.ternary",
				// {}
				"meta.block",
				"meta.braces",
				"meta.brace.curly",
				"punctuation.section.block.begin",
				"punctuation.section.block.end",
				"punctuation.section.braces.begin",
				"punctuation.section.braces.end ",
				"punctuation.section.scope",
				// []
				"meta.brackets",
				"meta.brace.square",
				"meta.array.empty",
				"punctuation.section.brackets.begin",
				"punctuation.section.brackets.end",
				"punctuation.section.array.begin",
				"punctuation.section.array.end",
				// <>
				"meta.generic",
				"punctuation.definition.generic.begin",
				"punctuation.definition.generic.end",
				"punctuation.definition.tag",
				// ()
				"meta.group",
				"meta.parens",
				"meta.brace.round",
				"punctuation.definition.arguments.begin",
				"punctuation.definition.arguments.end",
				"punctuation.definition.parameters",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				"punctuation.section.function",
				"punctuation.section.group.begin",
				"punctuation.section.group.end",
				"punctuation.section.parens.begin",
				"punctuation.section.parens.end",
				// ->, =>, or ::
				"punctuation.accessor",
				"storage.type.function.arrow",
				// Commas & Colons
				"meta.delimiter.comma",
				"meta.function.method.with-arguments",
				"punctuation.separator",
				// Semicolons
				"punctuation.terminator",
				// Other
				"meta.cast",
				"punctuation.definition.begin",
				"punctuation.definition.end",
				"source",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		/*
			COMMENTS
		*/
		{
			"name": "Comment",
			"scope": [
				"comment",
				"comment.block",
				"comment.block.documentation",
				"comment.line",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#666666"
			}
		},

		/*
			STRINGS
		*/
		{
			"name": "Strings & Keys",
			"scope": [
				"meta.group.regexp",
				"meta.structure.dictionary.json string.quoted.double.json",
				"string",
				"string.regexp",
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.capture.regexp",
				// Property names and attributes
				"constant.language.symbol.hashkey",
				"meta.property-name",
				"support.type.property-name",
				"variable.other.member",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#A270FF"
			}
		},
		/*
			CLASSES
		 */
		 {
			"name": "Current Class",
			"scope": [
				"entity.name.type.class",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0090F0"
			}
		},
		{
			"name": "Parent Classes",
			"scope": [
				"entity.name.type.module",
				"entity.name.class",
				"entity.other.inherited-class",
				"meta.module",
				"meta.class",
				"meta.other.inherited-class",
				"meta.path",
